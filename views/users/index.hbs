                <div class="user-left-min-content bac-color">
                    <div class="user-left-min-profil">
                        <div class="profil-home-page">
                            {{#each users}}
                            <div class="user-img-prf-box">
                                <div class="user-img-prf">
                                    <img src="/images/1639314204182.jfif" alt="">
                                    <div></div>
                                </div>
                            </div>
                            <div class="context-prf-user">
                                <h2 class="user-name-prf">{{name}}</h2>
                                <p class="user-direction-prf">Pega developer</p>
                                <div><span>Uzbekistan,Tashkent</span></div>
